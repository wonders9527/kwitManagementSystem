<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kwit.model.RecordProductConsumption" table="record_product_consumption" catalog="kwitmanagementsystem">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="socialCreditCode" type="java.lang.String">
            <column name="social_credit_code" length="18" not-null="true">
                <comment>统一社会信息代码</comment>
            </column>
        </property>
        <property name="storeName" type="java.lang.String">
            <column name="store_name" length="50" not-null="true">
                <comment>门店名称</comment>
            </column>
        </property>
        <property name="number" type="java.lang.String">
            <column name="number" length="30" not-null="true">
                <comment>编号</comment>
            </column>
        </property>
        <property name="productClassification" type="java.lang.String">
            <column name="product_classification" length="30" not-null="true">
                <comment>商品分类</comment>
            </column>
        </property>
        <property name="productName" type="java.lang.String">
            <column name="product_name" length="50" not-null="true">
                <comment>商品名称</comment>
            </column>
        </property>
        <property name="productSpecification" type="java.lang.String">
            <column name="product_specification" length="5" not-null="true">
                <comment>商品规格</comment>
            </column>
        </property>
        <property name="productOriginalPrice" type="java.lang.String">
            <column name="product_original_price" length="30" not-null="true">
                <comment>商品原价</comment>
            </column>
        </property>
        <property name="discountPrice" type="java.lang.String">
            <column name="discount_price" length="15" not-null="true">
                <comment>折扣价格</comment>
            </column>
        </property>
        <property name="quantity" type="java.lang.String">
            <column name="quantity" length="30" not-null="true">
                <comment>数量</comment>
            </column>
        </property>
        <property name="amount" type="java.lang.String">
            <column name="amount" length="30" not-null="true">
                <comment>金额</comment>
            </column>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status" length="30" not-null="true">
                <comment>状态</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="100">
                <comment>备注</comment>
            </column>
        </property>
        <property name="createDate" type="java.sql.Timestamp">
            <column name="Create_date" length="19" not-null="true">
                <comment>数据创建时间</comment>
            </column>
        </property>
        <property name="temp1" type="java.lang.String">
            <column name="Temp1" length="100">
                <comment>预留一</comment>
            </column>
        </property>
        <property name="temp2" type="java.lang.String">
            <column name="Temp2" length="100">
                <comment>预留二</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
