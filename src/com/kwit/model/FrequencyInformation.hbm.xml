<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kwit.model.FrequencyInformation" table="frequency_information" catalog="kwitmanagementsystem">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="socialCreditCode" type="java.lang.String">
            <column name="social_credit_code" length="18" not-null="true">
                <comment>统一社会信息代码</comment>
            </column>
        </property>
        <property name="storeName" type="java.lang.String">
            <column name="store_name" length="50" not-null="true">
                <comment>门店名称</comment>
            </column>
        </property>
        <property name="frequencyName" type="java.lang.String">
            <column name="frequency_name" length="30" not-null="true">
                <comment>次卡名称</comment>
            </column>
        </property>
        <property name="frequencyTimes" type="java.lang.String">
            <column name="frequency_times" length="10" not-null="true">
                <comment>次数</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="100">
                <comment>备注</comment>
            </column>
        </property>
        <property name="createDate" type="java.sql.Timestamp">
            <column name="create_date" length="19" not-null="true">
                <comment>数据创建时间</comment>
            </column>
        </property>
        <property name="temp1" type="java.lang.String">
            <column name="Temp1" length="100">
                <comment>预留一</comment>
            </column>
        </property>
        <property name="temp2" type="java.lang.String">
            <column name="Temp2" length="100">
                <comment>预留二</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
